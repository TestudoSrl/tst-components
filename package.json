{
  "name": "tst-tags",
  "version": "1.0.0",
  "description": "Testudo tags component build with mui",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.tsx\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git=https://github.com/TestudoSrl/tst-tags.git"
  },
  "keywords": [
    "tag",
    "mui",
    "testudo"
  ],
  "author": "Arash Nouri",
  "license": "ISC",
  "homepage": "",
  "devDependencies": {
    "@types/react": "^18.0.26",
    "prettier": "^2.8.1",
    "react-dom": "^18.2.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@mui/material": "^5.10.17",
    "react": "^18.2.0"
  },
  "files": [
    "dist/**/*"
  ],
  "engines": {
    "node": ">=10",
    "pnpm": ">=3"
  }
}
